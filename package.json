{
  "name": "vite-plugin-ol-external",
  "version": "0.0.1",
  "description": "external openlayers plugin for vite",
  "author": "aliothor",
  "repository": {
    "type": "git",
    "url": "https://github.com/aliothor/vite-plugin-ol-external"
  },
  "homepage": "https://github.com/aliothor/vite-plugin-ol-external",
  "bugs": "https://github.com/aliothor/vite-plugin-ol-external/issues",
  "license": "MIT",
  "scripts": {
    "dev": "tsc -w",
    "build": "rimraf dist && tsc && pnpm build:es",
    "build:es": "tsc --module ESNEXT --outDir dist/es",
    "lint:fix": "eslint --fix ./src/**/*.ts",
    "test": "vitest",
    "release": "pnpm build && bumpp --push --tag --commit && npm publish"
  },
  "files": [
    "dist",
    "index.d.ts"
  ],
  "keywords": [
    "vite-plugin",
    "external",
    "openlayers"
  ],
  "types": "index.d.ts",
  "main": "dist/src/index.js",
  "module": "dist/es/src/index.js",
  "peerDependencies": {
    "vite": ">=4.0.0"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "devDependencies": {
    "@types/estree": "^1.0.2",
    "@types/fs-extra": "^11.0.2",
    "@types/node": "^20.8.4",
    "@types/ws": "^8.5.6",
    "bumpp": "^9.2.0",
    "eslint": "^8.51.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.0.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "acorn": "^8.10.0",
    "es-module-lexer": "^1.3.1",
    "fs-extra": "^11.1.1",
    "magic-string": "^0.30.4"
  }
}
